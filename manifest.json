{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com 'unsafe-eval'; object-src 'self'",
  "name": "Bookmarks",
  "description": "Bookmark extension",
  "version": "1.0",
  "homepage_url": "http://www.codextension.io",
  "icons": {
    "128": "images/logo.png",
    "16": "images/logo.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "omnibox": { "keyword" : "bm" },
  "chrome_url_overrides": {
    "newtab": "bookmark.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "bookmarks",
    "activeTab",
    "declarativeContent",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ]
}
